{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "import/no-extraneous-dependencies": [
    "error", {
       "devDependencies": false,
       "optionalDependencies": false,
       "peerDependencies": false,
       "packageDir": "./"
    }
  ],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
 "rules": {
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  }
}
